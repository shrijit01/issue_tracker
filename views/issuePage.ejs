
    
    <!-- <button id="create-issue-button">Create Issue</button> -->

    <ul class="issue-list">
        <%foundIssue.forEach(function(issue){%>
            <!-- //this is for only logged in user issue  -->
                <li class="issue-item">
                    <h2>Title:- <%= issue.title %></h2>
                    <p>Description:- <%= issue.description %></p>
                    <p>Label:- <%= issue.label %></p>
                    <p>Author:- <%= issue.user.name %></p>
                    <p>Project :- <%= issue.project.name %></p>
                </li>
        <%})%>
    </ul>
